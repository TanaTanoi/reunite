<h1>My plans</h1>
<%= button_to "Create new plan", new_plan_path, method: "get" %>
<% @plans.each do |plan| %>
  <%= plan.name %>  <span class="glyphicon glyphicon-user" aria-hidden="true"></span> <%= plan.users.count.to_s() %>
  <% byebug %>
  <%= button_to "View plan", plan_path(plan), method: "get"%>
<% end %>

